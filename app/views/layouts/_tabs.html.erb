<div id="tabsWrapper">
<% if !@tabbed_navigation_items.nil? %>
    <ul id="tabs">
<% @tabbed_navigation_items.each do |item| -%>
            <li id="<%= "item_#{item[:id]}" %>" <% if item[:id] == @selected_navigation_item %> class='active'<% end %>>
            <a href="<%= item[:url] %>"><%= item[:id].l %></a>
            </li>
<% end %>
    </div>
<% end %>
    
<% if !@user_navigation_items.nil? %>
    <ul id="userTabs">
<% @user_navigation_items.each do |item| -%>
            <li id="<%= "item_#{item[:id]}" %>" <% if item[:id] == @selected_navigation_item %> class='active'<% end %>>
            <a href="<%= item[:url] %>"><%= item[:id].l %></a>
            </li>
<% end %>
    </div>
<% end %>
</div>